{
    "name": "omnichannel-connect-bemol",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "tsc -p tsconfig.build.json",
        "start": "npm run prisma:migrate:deploy && node dist/src/server.js",
        "dev:migration:deploy": "npm run dev:load-envs prisma migrate deploy",
        "dev:migration": "npm run dev:load-envs prisma migrate dev",
        "dev:migration:reset": "npm run dev:load-envs prisma migrate reset",
        "dev:load-envs": "dotenv -e .env.development",
        "prisma:generate": "prisma generate",
        "dev:seed": "npm run dev:load-envs prisma db seed",
        "dev": "cross-env NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts",
        "test:migration:deploy": "npm run test:load-envs prisma migrate deploy",
        "test:migration": "npm run test:load-envs prisma migrate dev",
        "test:migration:reset": "npm run test:load-envs prisma migrate reset",
        "test:watch": "cross-env NODE_ENV=test jest --watch --passWithNoTests --runInBand",
        "test:load-envs": "dotenv -e .env.test",
        "test:seed": "npm run test:load-envs prisma db seed",
        "test": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leonanm03/Omnichannel-Connect.git"
    },
    "keywords": [
        "backend",
        "bemol",
        "omnichannel",
        "tech-challenge",
        "javascript",
        "typescript",
        "docker",
        "postgres",
        "prisma"
    ],
    "author": "Leonan Martins",
    "license": "MPL-2.0",
    "bugs": {
        "url": "https://github.com/leonanm03/Omnichannel-Connect/issues"
    },
    "homepage": "https://github.com/leonanm03/Omnichannel-Connect#readme",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/node": "^20.5.0",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^7.3.0",
        "eslint": "^8.47.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-import-resolver-typescript": "^3.6.0",
        "eslint-plugin-boundaries": "^3.2.0",
        "eslint-plugin-import": "^2.28.0",
        "eslint-plugin-prettier": "^5.0.0",
        "git-commit-msg-linter": "^5.0.4",
        "jest": "^29.6.2",
        "lint-staged": "^14.0.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.2",
        "prisma": "^5.1.1",
        "supertest": "^6.3.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "ttypescript": "^1.5.15",
        "typescript": "^5.1.6",
        "typescript-transform-paths": "^3.4.6"
    },
    "dependencies": {
        "@prisma/client": "^5.1.1",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "dotenv-expand": "^10.0.0",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "http-status": "^1.6.2",
        "joi": "^17.9.2",
        "reflect-metadata": "^0.1.13"
